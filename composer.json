{
    "name": "meanify-co/laravel-obfuscator",
    "description": "A PHP library to obfuscate IDs in Laravel",
    "authors": [
        {
            "name": "Daniel Barcelos",
            "email": "daniel@meanify.co"
        }
    ],
    "keywords": [
        "utils",
        "helpers",
        "php",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "vinkla/hashids": "^13.0"
    },
    "require-dev": {
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^12.3",
        "illuminate/container": "^12.3"
    },
    "autoload": {
        "psr-4": {
            "Meanify\\LaravelObfuscator\\": "src/",
            "Meanify\\LaravelObfuscator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meanify\\LaravelObfuscator\\Providers\\MeanifyLaravelObfuscatorServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
